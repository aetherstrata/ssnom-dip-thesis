@Article{ma_2021,
	author={Ma, Xuezhi and Liu, Qiushi and Yu, Ning and Xu, Da and Kim, Sanggon and Liu, Zebin and Jiang, Kaili and Wong, Bryan M. and Yan, Ruoxue and Liu, Ming},
	title={6{\thinspace}nm super-resolution optical transmission and scattering spectroscopic imaging of carbon nanotubes using a nanometer-scale white light source},
	journal={Nature Communications},
	year={2021},
	month={11},
	day={25},
	volume={12},
	number={1},
	pages={6868},
	abstract={Optical transmission and scattering spectroscopic microscopy at the visible and adjacent wavelengths denote one of the most informative and inclusive characterization methods in material research. Unfortunately, restricted by the diffraction limit of light, it cannot resolve the nanoscale variation in light absorption and scattering, diagnostics of the local inhomogeneity in material structure and properties. Moreover, a large quantity of nanomaterials has anisotropic optical properties that are appealing yet hard to characterize through conventional optical methods. There is an increasing demand to extend the optical hyperspectral imaging into the nanometer length scale. In this work, we report a super-resolution hyperspectral imaging technique that uses a nanoscale white light source generated by superfocusing the light from a tungsten-halogen lamp to simultaneously obtain optical transmission and scattering spectroscopic images. A 6-nm spatial resolution in the visible to near-infrared wavelength regime (415--980{\thinspace}nm) is demonstrated on an individual single-walled carbon nanotube (SW-CNT). Both the longitudinal and transverse optical electronic transitions are measured, and the SW-CNT chiral indices can be identified. The band structure modulation in a SW-CNT through strain engineering is mapped.},
	issn={2041-1723},
	doi={10.1038/s41467-021-27216-5},
	url={https://doi.org/10.1038/s41467-021-27216-5}
}

