@ARTICLE{vobornik_2008,
	title     = {Scanning Near-field Optical Microscopy},
	author    = {Vobornik, Du{\v s}an and Vobornik, Slavenka},
	abstract  = {An average human eye can see details down to 0,07 mm in size.
