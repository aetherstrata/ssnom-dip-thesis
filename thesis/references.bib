	language  = {en}
}

@ARTICLE{mclaughlin_2004,
	title     = {The use of mutual information in registration of CT and MRI datasets post permanent implant},
	author    = {McLaughlin, Patrick W and Narayana, Vrinda and Kessler, Marc and McShan, Daniel and Troyer, Sara and Marsh, Lon and Hixson, George and Roberson, Peter L},
	abstract  = {PURPOSE: To determine the feasibility of registration of MRI and
